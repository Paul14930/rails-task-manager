<h1>All Tasks</h1>
<ul>
<% @tasks.each do |task|%>
  <li><%= link_to task.title,
    task_path(task.id) %></li>
    <li> <%= task.completed  %></li>
  <li><%= link_to "edit", edit_task_path(task) %></li>
  <li><%= link_to "delete", destroy_task_path(task) %></li>
<% end %>
</ul>
<%= link_to "Nouvelle tache", new_task_path %>

